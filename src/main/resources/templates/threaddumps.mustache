<!doctype html>
<html lang="en">
  <head>
      <title>Thread Dump</title>
      <link rel="stylesheet" href="/css/lib/bootstrap.min.css">
      <link type="text/css" rel="stylesheet" href="/css/emissary.css"/>
  </head>

  <body>
     <header></header>
     <main class="container">
        <h2>Thread Dump</h2>
        <strong>Emissary Version:</strong> {{ emissary.version }}<br />
        <strong>JVM Version:</strong> {{ java.version }} <br />
        <strong>JVM Name:</strong> {{ java.name }} <br />
        <strong>Thread count:</strong> current= {{ threadcount.current }}, max= {{ threadcount.max }}, daemon= {{ threadcount.daemon }}<br />
        <hr/>
        <h3>Deadlocked Threads:</h3>
        {{#deadlocks}}
          <pre>{{ stack }}</pre>
        {{/deadlocks}}
        <hr/>
        <h3>Thread Dump:</h3>
        {{#threads}}
          <pre>{{ stack }}</pre>
        {{/threads}}
     </main>

      <script src="/js/lib/jquery.min.js"></script>
      <script src="/js/lib/popper.min.js"></script>
      <script src="/js/lib/bootstrap.min.js"></script>
      <script src="/js/emissary.js" type="text/javascript" language="javascript"></script>
  </body>
</html>
